<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="hello" class="com.prowings.Hello">
		<property name="massage" value="Jay Shree Ram!!!"></property>
	</bean>

	<bean id="std" class="com.prowings.Student">
		<property name="roll" value="101" />
		<property name="name" value="RadheSham" />
		<property name="city" value="Mathura" />
	</bean>
	
	<bean id="emp" class="com.prowings.Employee">
		<constructor-arg value="012" type="int"/>
		<constructor-arg value="Ramu" type="String"/>
		<constructor-arg value="pune" type="String"/>
	</bean>
</beans>
   
   